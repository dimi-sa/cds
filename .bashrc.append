
cds_path=$HOME/.local/bin/cds

cds(){
case $1 in
  add | addwd | del | list ) $cds_path $@;;
  "") $cds_path list;;
  *) cd_to_path $1
esac
}

cd_to_path(){
dir=$(awk -F, '$1 ~ /^'$1'$/{print $2}' $HOME/.dir_nicknames)
case $dir in
  "") echo "I don't know that nickname :/";;
  *) $cds_path inc_cd_counter $1; cd $dir
esac
}
